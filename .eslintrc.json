{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/destructuring-assignment":"off",
        "react/prop-types": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "arrow-body-style":"off",
        "react/jsx-one-expression-per-line": "off",
        "no-useless-catch": "off",
        "no-shadow": "off",
        "no-unused-vars": "off",
        "no-alert":"off"
    }
}
